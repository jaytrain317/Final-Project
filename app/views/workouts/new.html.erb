<h1>Create New Workout Log</h1>

    
    <%= form_for @workouts do |f| %>
    
    <p>
        <%= f.label :weight %> </br>
        <%= f.number_field :weight %>
    </p>
    <p>
        <%= f.label :exercise %> </br>
        <%= f.text_field :exercise %> 
    </p>
    
    <p>
        <%= f.label :repetition %> </br>
        <%= f.number_field :repetition %>
    </p>
    
    <p>
        <%= f.label :time %> </br>
        <%= f.number_field :time %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
    
    <%end%>